{
    "expo": {
        "name": "Memora",
        "slug": "memora",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "memora",
        "userInterfaceStyle": "dark",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#0f0f0f"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "app.memora.community",
            "infoPlist": {
                "NSMicrophoneUsageDescription": "Memora uses the microphone for voice input.",
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "package": "app.memora.community",
            "versionCode": 1,
            "permissions": [
                "android.permission.RECORD_AUDIO"
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#0f0f0f"
            },
            "statusBar": {
                "backgroundColor": "#0f0f0f",
                "barStyle": "light-content"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-notifications",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "openaiApiKey": "",
            "router": {}
        }
    }
}